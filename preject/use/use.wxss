.container{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.number,.endride{
  width: 100%;
  flex: 2;
  text-align: center;
}
.time{
  flex: 6;
  width : 100%;
  text-align: center;
}
.time .time-content {
  font-size: 100rpx;
}
.endride button{
  width: 90%;
  margin-top: 40rpx;
}